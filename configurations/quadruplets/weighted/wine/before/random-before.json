{
  "name": "WeightedBeforeRandomSamp",
  "type": "quadruplet",
  "dataset": {
    "dataset_name": "wine",
    "dataset_path": "datasets/clean_data/official_datasets/wine.csv",
    "majority_classes": [
      5,
      6
    ],
    "undersample": "True",
    "undersample_method": "random_undersampling"
  },
  "model": {
    "epochs": 30,
    "batch_size": 32,
    "layers_config": [
      11,
      128,
      64,
      12
    ]
  },
  "model_params": {
    "start_epoch": 1,
    "load_params": "False"
  },
  "scheduler": {
    "step_size": 1,
    "gamma": 0.99
  },
  "optimizer": {
    "learning_rate": 0.003
  },
  "classifier": {
    "k": 5
  },
  "clustering": {
    "k": 3
  },
  "epoch_log_interval": 10,
  "weights_enabled": "True",
  "checkpoint_path": "./results/quadruplet/weighted/wine/RandomSampBefore",
  "save_name": "WeightedBeforeRandomSamp"
}
