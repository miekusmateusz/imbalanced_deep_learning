{
  "name": "NonWeightedEditBefore",
  "type": "quadruplet",
  "dataset": {
    "dataset_name": "a4",
    "dataset_path": "datasets/clean_data/official_datasets/a4.csv",
    "majority_classes": [
      2
    ],
    "undersample": "True",
    "undersample_method": "edited_nearest_neighbour"
  },
  "model": {
    "epochs": 30,
    "batch_size": 16,
    "layers_config": [
      2,
      128,
      64,
      8
    ]
  },
  "model_params": {
    "start_epoch": 1,
    "load_params": "False"
  },
  "scheduler": {
    "step_size": 1,
    "gamma": 0.99
  },
  "optimizer": {
    "learning_rate": 0.01
  },
  "classifier": {
    "k": 5
  },
  "clustering": {
    "k": 3
  },
  "epoch_log_interval": 10,
  "weights_enabled": "False",
  "checkpoint_path": "./results/quadruplet/non-weighted/a4/EditBefore",
  "save_name": "NonWeightedEditBefore"
}
