  "classifier": {
    "k": 5
  },
  "clustering": {
    "k": 3
  },
  "epoch_log_interval": 10,
  "weights_enabled": "True",
  "checkpoint_path": "./results/triplet/weighted/ecoli/Default",
  "save_name": "Default"
