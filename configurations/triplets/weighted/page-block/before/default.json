    "load_params": "False"
  },
  "scheduler": {
    "step_size": 1,
    "gamma": 0.99
  },
  "optimizer": {
    "learning_rate": 0.003
  },
  "classifier": {
    "k": 5
  },
  "clustering": {
    "k": 3
  },
  "epoch_log_interval": 10,
  "weights_enabled": "True",
  "checkpoint_path": "./results/triplet/weighted/page_block/Default",
  "save_name": "Default"
